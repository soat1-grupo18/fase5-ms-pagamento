aws.region=${AWS_REGION}
aws.dynamodb.endpoint=${AWS_DYNAMODB_ENDPOINT}
aws.dynamodb.accessKey=${AWS_ACCESS_KEY}
aws.dynamodb.secretKey=${AWS_SECRET_KEY}
logging.level.root=DEBUG
logging.level.org.springframework=DEBUG
hibernate.validator.annotation-processor.ignore-explicit-configuration=true
spring.jpa.properties.javax.persistence.validation.mode=none
server.port=8083

queue.name.pedido-recebido=pedidoRecebido
spring.cloud.aws.region.static=${AWS_REGION}
spring.cloud.aws.credentials.access-key=${AWS_ACCESS_KEY}
spring.cloud.aws.credentials.secret-key=${AWS_SECRET_KEY}
spring.cloud.aws.sqs.endpoint=${AWS_SQS_ENDPOINT}

spring.data.dynamodb.entity2ddl.auto=create
spring.data.dynamodb.entity2ddl.gsiProjectionType=ALL
spring.data.dynamodb.entity2ddl.readCapacity=10
spring.data.dynamodb.entity2ddl.writeCapacity=1